	<!-- Class: <<XSDsimpleType>> Decimal13P2Type  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="Decimal13P2Type">
		<restriction base="decimal">
			<fractionDigits value="2"/>
			<totalDigits value="13"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> Decimal5P2Type  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="Decimal5P2Type">
		<restriction base="decimal">
			<fractionDigits value="2"/>
			<totalDigits value="5"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> Percent5P2Type  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="Percent5P2Type">
		<restriction base="dt:Decimal5P2Type">
			<minInclusive value="0.00"/>
			<maxInclusive value="100"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> Int2Type  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="Int2Type">
		<restriction base="int">
