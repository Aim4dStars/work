	<!-- Package: <<XSDschema>> BaseDataTypes  -->
	<!--  - Verson 1 (Base Team): Initial version for Base project -->
	<!--  - Verson 2 (8===8): New text types and Barcode for Life Compliance project -->
	<!--  - Verson 3 (8===8): New text types for Priority Platform project -->
	<!-- =========================================================== -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- ~~ OTHER TYPES                                                                            ~~ -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> BSBType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="BSBType">
		<restriction base="string">
			<pattern value="\d{3}-\d{3}"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> BTePBarcodeType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="BTePBarcodeType">
		<restriction base="string">
			<maxLength value="10"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> TransactionReferenceNumberType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="TransactionReferenceNumberType">
		<annotation>
			<documentation>UUID followed by optional subtransaction/s i.e. UUID.Sn.Sn... where n represents number of subtransaction</documentation>
		</annotation>
		<restriction base="string">
			<pattern value="[0-9a-fA-F]{8}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{4}-[0-9a-fA-F]{12}[.0-9S]{0,44}"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<!-- Class: <<XSDsimpleType>> MessageVersionType  -->
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
	<simpleType name="MessageVersionType">
		<restriction base="string">
			<pattern value="\d+_\d+"/>
		</restriction>
	</simpleType>
	<!-- ~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~~ -->
