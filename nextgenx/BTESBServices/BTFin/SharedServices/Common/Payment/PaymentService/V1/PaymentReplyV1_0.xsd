	<!-->>>>>>>>>UpdatePaymentInstruction<<<<<<<<<<-->
	<xs:element name="UpdatePaymentInstructionResponseMsg" type="payrsp:UpdatePaymentInstructionResponseMsgType"/>
	<xs:complexType name="UpdatePaymentInstructionResponseMsgType">
