BEGIN
   EXECUTE IMMEDIATE 'DROP TABLE acl_object_identity';
EXCEPTION
   WHEN OTHERS THEN
      IF SQLCODE != -942 THEN
         RAISE;
      END IF;
END;
/
BEGIN
   EXECUTE IMMEDIATE 'CREATE TABLE acl_object_identity (
                      id number(38,0) NOT NULL
                      ,  object_id_class number(38,0) NOT NULL
                      , object_id_identity number(38,0) NOT NULL
                      , parent_object number(38,0) DEFAULT NULL
                      , owner_sid number(38,0) DEFAULT NULL
                      ,  entries_inheriting smallint NOT NULL
                      , CONSTRAINT acl_object_identity_pk  PRIMARY KEY (id)
                      , CONSTRAINT uk_acl_objid UNIQUE (object_id_class,object_id_identity))';
END;
/
