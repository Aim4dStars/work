<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE tiles-definitions PUBLIC
        "-//Apache Software Foundation//DTD Tiles Configuration 2.0//EN"
        "http://tiles.apache.org/dtds/tiles-config_2_0.dtd">
<tiles-definitions>
	<definition name="singlePageApplication" template="/WEB-INF/jsp/spa.jsp"/>
    <!--Desktop common layout-->
    <!--It will prepare global elements in header section, we may need to change it only for header section
        once the content is moved from commonLayout.jsp to header.jsp-->

    <definition name="cash.commonLayout" preparer="com.bt.nextgen.core.web.controller.GlobalElementPreparer"
                template="/WEB-INF/jsp/layout/commonLayout.jsp">
        <put-attribute name="pageHead" value="/WEB-INF/jsp/layout/pageHead.jsp"/>
        <put-attribute name="title" value=""/>
        <put-attribute name="header" value="/WEB-INF/jsp/cash/header.jsp"/>
        <put-attribute name="detailView" value=""/>
        <put-attribute name="body" value=""/>
    </definition>

    <definition name="unAuthLayout" template="/WEB-INF/jsp/layout/unAuthLayout.jsp">
        <put-attribute name="pageHead" value="/WEB-INF/jsp/layout/pageHead.jsp"/>
        <put-attribute name="body" value=""/>
    </definition>

    <definition name="unAuthLayout.tablet" template="/WEB-INF/jsp/layout/unAuthLayout.tablet.jsp">
        <put-attribute name="pageHead" value="/WEB-INF/jsp/layout/pageHead.tablet.jsp"/>
        <put-attribute name="body" value=""/>
    </definition>
</tiles-definitions>