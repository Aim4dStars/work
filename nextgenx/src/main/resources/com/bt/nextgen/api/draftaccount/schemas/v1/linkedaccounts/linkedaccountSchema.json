    "bsb": {
      "type": "string",
      "description": "LinkedAccountForm.getBsb"
    },
    "accountnumber": {
      "type": "string",
      "description": "LinkedAccountForm.getAccountNumber"
    },
    "nickname": {
      "type": "string",
      "description": "LinkedAccountForm.getNickName"
    },
    "directdebitamount": {
      "type": "string",
      "description": "LinkedAccountForm.getDirectDebitAmount"
    },
    "bsbValidationResultVal": {
      "type": "boolean",
